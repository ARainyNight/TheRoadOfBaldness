<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

   
    <servlet>
        <servlet-name>ContentServlet</servlet-name>
        <servlet-class>com.nuc.ajax.ContentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContentServlet</servlet-name>
        <url-pattern>/content</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NewsListServlet</servlet-name>
        <servlet-class>com.nuc.ajax.NewsListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewsListServlet</servlet-name>
        <url-pattern>/news</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>department</servlet-name>
        <servlet-class>com.nuc.oa.DepartmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>department</servlet-name>
        <url-pattern>/department</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>po</servlet-name>
        <servlet-class>com.nuc.oa.PoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>po</servlet-name>
        <url-pattern>/po</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>staff</servlet-name>
        <servlet-class>com.nuc.oa.StaffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>staff</servlet-name>
        <url-pattern>/staff</url-pattern>
    </servlet-mapping>


</web-app>